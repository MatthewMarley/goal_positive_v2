<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <h1 align="center">Users</h1>
        <% @users.each do |user| %>
            <div class="well">
                <strong><%= link_to user.username, user_path(user) %></strong>
                <%= pluralize(user.chains.count, "chains") %>
            </div>
        <% end %>
    </div>
</div>