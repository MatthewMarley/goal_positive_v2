<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <% @categories.each do |category| %>
            <div class="well">
                <strong><%= link_to category.name, category_path(category) %></strong>
                <%= category.created_at %>
                </br>
                <%= pluralize(category.chains.count, "chain") %>
            </div>
        <% end %>
    </div>
</div>